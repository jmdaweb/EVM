void echocheck(int dir)
{
	string ptyle = get_tile_at(me.x, me.y);
	if (ptyle == "" && jumping == 0 && pulou == 0 && mapname != "túnel" && !dcheck && !echeck && moveable == true)
	{
		echotimer.restart();
		pulou = 1;
		echo = false;
		send_reliable(peer_id, "draw caiu.ogg", 0);
		p.play_stationary("caiu.ogg", false);
		return;
	}
}
