string pegartelhado(string mapa){
	file f;
	f.open("maps/"+mapa+".map", "r");
	string[] m=string_split(f.read(), "\r\n", false);
	for(uint i=0;i<m.length();i++){
		if(string_contains(m[i], "teto", 1)>-1){
			string[] mm=string_split(m[i], ":", false);
			return mm[1];
		}
	}
	return "rain4.ogg";
}
